{
  "name": "@guille/env",
  "version": "1.0.3",
  "exports": {
    ".": "./mod.ts",
    "./load": "./load.ts"
  },
  "tasks": {
    "test": "deno test --allow-env --allow-read --coverage",
    "publish-dry": "deno publish --dry-run"
  },
  "fmt": {
    "lineWidth": 100,
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": true
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^0.225.2",
    "@std/testing": "jsr:@std/testing@^0.224.0"
  },
  "publish": {
    "exclude": ["tests", "coverage", ".github"]
  }
}
